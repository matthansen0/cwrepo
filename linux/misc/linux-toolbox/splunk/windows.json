        },
        "ds_BE7eYAY9": {
            "name": "Succesful_Login_All_Users",
            "options": {
                "query": "source=WinEventLog:Security EventCode=4624\n| eval time=strftime(_time, \"%Y-%m-%d %H:%M:%S\") \n| where NOT like(user, \"%$\") AND NOT user=\"SYSTEM\"\n| table time, Logon_Type, host, user, src\n| sort - time\n"
            },
            "type": "ds.search"
        },
        "ds_E9sA92AH": {
            "name": "Added_Domain_Account",
            "options": {
                "query": "source=WinEventLog:Security EventCode=4720\n| eval time=strftime(_time, \"%Y-%m-%d %H:%M:%S\") \n| table time, host, user, src_user\n| sort - time"
            },
            "type": "ds.search"
        },
        "ds_I11I9uf4": {
            "name": "Domain_Replication",
            "options": {
                "query": "EventCode=4662 \"*{1131*\"\n| eval time=strftime(_time, \"%Y-%m-%d %H:%M:%S\") \n| table time, host, user\n| sort - time"
            },
            "type": "ds.search"
        },
        "ds_JtH62vNF": {
            "name": "AD_External_Access",
            "options": {
                "query": "sourcetype=pfirewall (dst_port=88 OR dst_port=3389 OR dst_port=389 OR dst_port=636 OR dst_port=3268 OR dst_port=3269 OR dst_port=445 OR dst_port=135 OR dst_port=137 OR dst_port=138 OR dst_port=139 OR dst_port=53) \nNOT (src_ip=\"192.168.220.*\" OR src_ip=\"fe80::*\" OR src_ip=\"::1\")\n| eval time=strftime(_time, \"%Y-%m-%d %H:%M:%S\") \n| table time, host, action, protocol, src_ip, src_port, dst_ip, dst_port\n| sort - time"
            },
            "type": "ds.search"
        },
        "ds_KmIYLAly": {
            "name": "Process_Creation",
            "options": {
                "query": "EventCode=4688 AND NOT (parent_process_name=splunkd.exe)\n| eval time=strftime(_time, \"%Y-%m-%d %H:%M:%S\") \n| table time, host, src_user, process, parent_process_name\n| sort - time"
            },
            "type": "ds.search"
        },
        "ds_L5DklzcC": {
            "name": "Registry_Editor",
            "options": {
                "query": "(EventCode=7036 OR EventCode=4688 OR EventCode=7045)\r\n(\"*RemoteRegistry*\" OR \"*regedit*\" OR \"*reg*\") "
            },
            "type": "ds.search"
        },
        "ds_LKeM5IT6": {
            "name": "All_Packets",
            "options": {
                "query": "sourcetype=pfirewall\n| eval time=strftime(_time, \"%Y-%m-%d %H:%M:%S\") \n| table time, host, action, protocol, src_ip, src_port, dst_ip, dst_port\n| sort - time"
